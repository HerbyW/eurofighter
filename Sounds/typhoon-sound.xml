		<engine1-fan-1>
			<name>Intake Fan 1</name>
			<mode>looped</mode>
			<path>Aircraft/eurofighter/Sounds/Samples/engine-intake.wav</path>
			<condition>
				<not>
					<property>sim/current-view/internal</property>
				</not>
				<equals>
					<property>controls/switches/lp-cock-left</property>
					<value>1</value>
				</equals>
			</condition>
			<position>
				<x>5.5</x>
				<y>0.5</y>
				<z>0.001</z>
			</position>
			<orientation>
				<x>-90.0</x>
				<y>0</y>
				<z>0</z>
				<inner-angle>110.0</inner-angle>
				<outer-angle>180.0</outer-angle>
				<outer-gain>0.4</outer-gain>
