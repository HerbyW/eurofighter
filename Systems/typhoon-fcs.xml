 <!--       -=+# Command Outputs #+=-      -->
 
 <!--         -= Pilot Commands =-         -->
	<filter>
		<name>Pilot Command: Pitch</name>
		<debug>false</debug>
		<type>gain</type>
		<enable>
			<condition>
				<equals>
					<property>/systems/autopilot/locks/altitude</property>
					<value></value>
				</equals>
			</condition>
		</enable>
		<input>
			<condition>
				<equals>
					<property>/controls/buttons/FCS-override</property>
					<value>true</value>
				</equals>
			</condition>
			<property>controls/flight/elevator</property>
		</input>
		<input>
			<condition>
				<equals>
					<property>/controls/buttons/FCS-override</property>
					<value>false</value>
				</equals>
			</condition>
			<property>/systems/FCS/control-inputs/elevator</property>
		</input>
		<output>
			<property>/systems/FCS/internal/pitch-command</property>
		</output>
		<gain>
			1.0
		</gain>
		<u_min>-1.0</u_min>
		<u_max>1.0</u_max>
	</filter>
	<filter>
		<name>Pilot Command: Vert Speed</name>
		<debug>false</debug>
		<type>gain</type>
		<enable>
			<condition>
				<equals>
					<property>/systems/autopilot/locks/altitude</property>
					<value></value>
				</equals>
			</condition>
		</enable>
		<input>
			<condition>
				<equals>
					<property>/controls/buttons/FCS-override</property>
					<value>true</value>
				</equals>
			</condition>
			<property>controls/flight/elevator-trim</property>
		</input>
		<input>
			<condition>
				<equals>
					<property>/controls/buttons/FCS-override</property>
					<value>false</value>
				</equals>
			</condition>
			<property>/systems/FCS/control-inputs/elevator-trim</property>
		</input>
		<output>
			<property>/systems/FCS/internal/canard-trim</property>
		</output>
		<gain>
			1.0
		</gain>
		<u_min>-1.0</u_min>
		<u_max>1.0</u_max>
	</filter>
	<filter>
		<name>Pilot Command: Roll</name>
		<debug>false</debug>
		<type>gain</type>
		<enable>
			<condition>
				<equals>
					<property>/systems/autopilot/locks/heading</property>
					<value></value>
				</equals>
			</condition>
		</enable>
		<input>
			<condition>
				<equals>
					<property>/controls/buttons/FCS-override</property>
					<value>true</value>
				</equals>
			</condition>
			<property>controls/flight/aileron</property>
		</input>
		<input>
			<condition>
				<equals>
					<property>/controls/buttons/FCS-override</property>
					<value>false</value>
				</equals>
			</condition>
			<property>/systems/FCS/control-inputs/aileron</property>
		</input>
		<output>
			<property>/systems/FCS/internal/roll-command</property>
		</output>
		<gain>
			1.0
		</gain>
		<u_min>-1.0</u_min>
		<u_max>1.0</u_max>
	</filter>
	<filter>
		<name>Pilot Command: Yaw</name>
		<debug>false</debug>
		<type>gain</type>
		<enable>
