		</input>
		<input>
			<condition>
				<equals>
					<property>/controls/buttons/FCS-override</property>
					<value>false</value>
				</equals>
			</condition>
			<property>/systems/FCS/control-inputs/rudder</property>
		</input>
		<output>
			<property>/systems/FCS/internal/yaw-command</property>
		</output>
		<gain>1.0</gain>
		<u_min>-1.0</u_min>
		<u_max>1.0</u_max>
	</filter>
 
 <!-- -= Hands Off / Autopilot Commands -= -->
	<pid-controller>
		<name>System Command: Pitch</name>
		<debug>false</debug>
		<enable>
			<condition>
				<not-equals>
					<property>/systems/autopilot/locks/altitude</property>
					<value />
				</not-equals>
			</condition>
		</enable>
		<input>
			<property>/orientation/pitch-deg</property>
		</input>
		<reference>
			<property>/systems/FCS/internal/target-pitch-deg</property>
		</reference>
		<output>
			<property>/systems/FCS/internal/pitch-command</property>
		</output>
		<config>
			<Kp>      <!-- proportional gain -->
				<condition>
					<or>
						<less-than>
							<property>/orientation/roll-deg</property>
							<value>-90</value>
						</less-than>
						<greater-than>
							<property>/orientation/roll-deg</property>
							<value>90</value>
						</greater-than>
					</or>
				</condition>
				<value>0.005</value>
			</Kp>
			<Kp>      <!-- proportional gain -->
				<condition>
					<and>
						<greater-than>
							<property>/orientation/roll-deg</property>
							<value>-90</value>
						</greater-than>
						<less-than>
							<property>/orientation/roll-deg</property>
							<value>90</value>
						</less-than>
					</and>
				</condition>
				<value>-0.05</value>
			</Kp>
			<beta>1.0</beta>    <!-- input value weighing factor -->
			<alpha>0.1</alpha>  <!-- low pass filter weighing factor -->
			<gamma>0.0</gamma>  <!-- input value weighing factor for -->
			<Ti>10.0</Ti>       <!-- integrator time  10.0 -->
			<Td>0.00001</Td>    <!-- derivator time -->
			<u_min>-0.4</u_min> <!-- minimum output clamp -->
			<u_max>0.4</u_max>
		</config>
	</pid-controller>
 
