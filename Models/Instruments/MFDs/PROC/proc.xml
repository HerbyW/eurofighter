<?xml version="1.0" encoding="utf-8"?>
<PropertyList>
	<path>proc.ac</path>

	<animation>
		<type>select</type>
		<object-name>canopy</object-name>
		<condition>
			<greater-than>
				<property>canopy/position-norm</property>
				<value>0</value>
			</greater-than>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>seatsafe</object-name>
		<condition>
			<not>
				<property>/controls/seat/arming-handle</property>
			</not>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>apustart</object-name>
		<condition>
			<and>
				<greater-than>
					<property>/engines/engine[2]/rpm</property>
					<value>1</value>
				</greater-than>
				<less-than>
					<property>/engines/engine[2]/rpm</property>
					<value>55</value>
				</less-than>
				<not-equals>
					<property>/controls/engines/engine[2]/cutoff</property>
					<value>true</value>
				</not-equals>
			</and>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>apurun</object-name>
		<condition>
			<greater-than>
				<property>/engines/engine[2]/rpm</property>
				<value>55</value>
			</greater-than>
		</condition>
	</animation>
